#. module: odoo_marketplace
#: model:mail.template,body_html:odoo_marketplace.marketplace_email_template_for_seller_status_change_to_admin
msgid ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"\n"
"                    <t t-if=\"object.state=='approved'\">\n"
"                        <p>Hello <t t-out=\"object.company_id and object.company_id.partner_id.name or ctx.get('company') and ctx.get('company').partner_id.name or object.user_ids and object.user_ids[0].company_id.name or ''\"></t>,</p>\n"
"\n"
"                        <p>Request of <t t-out=\"object.name\"></t> to become a seller on our shop has been approved.</p>\n"
"                    </t>\n"
"\n"
"                    <t t-if=\"object.state=='denied'\">\n"
"                        <p>Hello <t t-out=\"object.company_id and object.company_id.partner_id.name or ctx.get('company') and ctx.get('company').partner_id.name or object.user_ids and object.user_ids[0].company_id.name or ''\"></t>,</p>\n"
"\n"
"                        <p>Request of <t t-out=\"object.name\"></t> to become a seller on our shop has been denied.</p>\n"
"                    </t>\n"
"\n"
"                    <t t-if=\"object.state=='pending'\">\n"
