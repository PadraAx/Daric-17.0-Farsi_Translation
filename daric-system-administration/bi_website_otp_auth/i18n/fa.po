#. module: bi_website_otp_auth
#: model:mail.template,body_html:bi_website_otp_auth.send_otp_email_template
msgid ""
"<div style=\"padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777\">\n"
"                    <p>Dear <t t-out=\"ctx.get('partner_name')\"></t>,</p>\n"
"                    <p>Your OTP is: <t t-out=\"object.otp_name\"></t></p>\n"
"                    <p>\n"
"                        For any queries or clarifications, please e-mail us at <a t-attf-href=\"user.company_id.email\" target=\"_blank\"><t t-out=\"user.company_id.email\"></t></a> and we shall be glad to assist.\n"
"                    </p>\n"
"                    <p>Best regards,</p>\n"
"                </div>\n"
"                <div style=\"padding:0px;width:600px;margin:auto; margin-top: 10px; background: #fff repeat top /100%;color:#777777\">\n"
"                    <t t-out=\"user.company_id.name\"></t>\n"
"                </div>\n"
"            "
msgstr ""
"<div style = \"padding: 0px ؛ عرض: 600px ؛ حاشیه: خودکار ؛ زمینه: #ffffff تکرار بالا /100 ٪ ؛ رنگ: #777777\">\n"
"                    <p> عزیز <t-out = \"ctx.get ('partner_name')\"> </t> ، </p>\n"
"                    <p> OTP شما این است: <t t-out = \"object.otp_name\"> </t> </p>\n"
"                    <p>\n"
"                        برای هرگونه پرس و جو یا شفاف سازی ، لطفاً از طریق ایمیل در <a t-attf-href = \"user.com_id.email\" target = \"_ blank\"> <t t-out = \"user.com_id.email\"> </t> </a> از طریق ایمیل ارسال کنید و خوشحال می شویم.\n"
"                    </p>\n"
"                    <P> با احترام ، </p>\n"
"                </div>\n"
"                <div style = \"padding: 0px ؛ عرض: 600px ؛ حاشیه: auto ؛ margin-top: 10px ؛ پس زمینه: #fff تکرار بالا /100 ٪ ؛ رنگ: #777777\">\n"
"                    <t t-out = \"user.company_id.name\"> </t>\n"
"                </div>"

